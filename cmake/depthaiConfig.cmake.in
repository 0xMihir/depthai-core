# Get whether library was build as shared or not
set(depthai_SHARED_LIBS @BUILD_SHARED_LIBS@)

# Specify that this is config mode (Called by find_package)
set(CONFIG_MODE TRUE)

# Compute the installation prefix relative to this file.
set(_IMPORT_PREFIX "@DEPTHAI_DEPENDENCIES_INSTALLATION_PATH_REL@")

# Add dependencies file
include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Dependencies.cmake")

# Add the targets file
include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_EXPORT_GROUP@.cmake")

# Cleanup
set(_IMPORT_PREFIX)

# Add aliases
if(TARGET @PROJECT_NAME@::@TARGET_CORE_NAME@)
    add_library(@PROJECT_NAME@::@TARGET_CORE_ALIAS@ INTERFACE IMPORTED GLOBAL)
    set_property(TARGET @PROJECT_NAME@::@TARGET_CORE_ALIAS@ PROPERTY INTERFACE_LINK_LIBRARIES @PROJECT_NAME@::@TARGET_CORE_NAME@)
endif()
if(TARGET @PROJECT_NAME@::@TARGET_OPENCV_NAME@)
    add_library(@PROJECT_NAME@::@TARGET_OPENCV_ALIAS@ INTERFACE IMPORTED GLOBAL)
    set_property(TARGET @PROJECT_NAME@::@TARGET_OPENCV_ALIAS@ PROPERTY INTERFACE_LINK_LIBRARIES @PROJECT_NAME@::@TARGET_OPENCV_NAME@)
endif()
